<!-- ======================================================================
     19/01/2018  Erwan MorÃ©ac
     Adding of HPeC sensors: 
     - 2 cameras 
     - 1 light sensor (based on a camera, 08/01/19)
     - 6 short range infrared sensors
     - 6 long range infrared sensors
     - 6 ultrasound sensors
======================================================================= -->

<!-- ===================== Add of the 2 cameras  =====================-->
  <link name='iris/camera1_link'>
    <pose>0 0 -.07 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00002</iyy>
          <iyz>0</iyz>
          <izz>0.00002</izz>
        </inertia>
      </inertial>
    <collision name='camera1_link_collision'>
      <geometry>
        <box>
            <size>0.05 0.05 0.05</size>
        </box>
      </geometry>
    </collision>
    <visual name='camera1_link_visual'>
      <geometry>
        <box>
            <size>0.05 0.05 0.05</size>
        </box>
      </geometry>
    </visual>
    <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/FlatBlack</name>
          </script>
    </material>
    <sensor name='camera1_sensor' type='camera'>
      <update_rate>30.0</update_rate>
      <camera name='head'>
        <horizontal_fov>1.0472</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>L8</format>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
      </camera>
      <plugin name='camera_controller' filename='libgazebo_ros_camera.so'>
        <robotNamespace>/</robotNamespace>
        <alwaysOn>true</alwaysOn>
        <updateRate>0.0</updateRate>
        <cameraName>iris/camera1</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>camera</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </link>
  <joint name='iris/camera1_joint' type='fixed'>
    <child>iris/camera1_link</child>
    <parent>base_link</parent> 
  </joint>

